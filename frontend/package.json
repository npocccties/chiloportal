{
  "name": "frontend",
  "description": "frontend of chiloportal",
  "version": "0.0.0",
  "author": "Kimiaki Kuno <knokmki612@gmail.com>",
  "bugs": "https://github.com/npocccties/chiloportal/issues",
  "dependencies": {
    "next": "^12.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@aspida/fetch": "^1.11.0",
    "@headlessui/react": "^1.6.6",
    "@iconify/react": "^3.2.2",
    "@jumpu-ui/tailwindcss": "^1.0.1-alpha.5",
    "@types/node": "^18.7.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "aspida": "^1.11.0",
    "autoprefixer": "^10.4.8",
    "clsx": "^1.2.1",
    "eslint": "^8.23.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "fixpack": "^4.0.0",
    "minimist": "^1.2.6",
    "msw": "^0.47.0",
    "npm-run-all": "^4.1.5",
    "openapi2aspida": "^0.19.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "yn": "^5.0.0"
  },
  "engines": {
    "node": "^16.16.0"
  },
  "homepage": "https://github.com/npocccties/chiloportal#readme",
  "keywords": [
    "Open Badge"
  ],
  "main": "",
  "packageManager": "yarn@3.2.2",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npocccties/chiloportal.git"
  },
  "scripts": {
    "api": "run-s api:clean api:build",
    "api:build": "openapi2aspida -i ../backend/doc/openapi.yaml",
    "api:clean": "rimraf api",
    "build": "run-s api build:clean build:app",
    "build:app": "next build",
    "build:clean": "rimraf public/mockServiceWorker.js",
    "dev": "run-s api dev:mock dev:app",
    "dev:app": "next dev",
    "dev:mock": "msw init public --no-save",
    "format": "run-s format:code format:package",
    "format:code": "prettier --write .",
    "format:package": "fixpack",
    "lint": "next lint",
    "start": "next start",
    "update": "yarn up '*' && yarn up --recursive '*' && yarn dedupe"
  }
}
