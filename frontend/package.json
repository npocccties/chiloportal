    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "aspida": "^1.11.0",
    "clsx": "^1.2.1",
    "image-size": "^1.0.2",
    "jotai": "^1.9.0",
    "msw": "^0.47.4",
    "next": "^13.0.0",
    "next-mdx-remote": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.31.1",
    "swr": "^1.3.0",
    "to-vfile": "^7.2.3",
    "unist-util-visit": "^4.1.1",
    "vfile": "^5.3.5",
    "vfile-matter": "^4.0.0",
    "yaml": "^2.1.3",
    "yn": "^5.0.0"
  },
  "devDependencies": {
    "@jumpu-ui/tailwindcss": "^1.0.1-alpha.5",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "^18.7.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-canonical": "^3.1.0",
    "fixpack": "^4.0.0",
    "json-schema-to-ts": "^2.5.5",
    "npm-run-all": "^4.1.5",
    "openapi2aspida": "^0.19.0",
    "pathpida": "^0.18.1",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "react-textarea-autosize": "^8.4.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4",
    "unified": "^10.1.2"
  },
  "engines": {
    "node": "^16.18.0"
  },
  "homepage": "https://github.com/npocccties/chiloportal#readme",
  "keywords": [
    "Open Badge"
  ],
  "main": "",
  "packageManager": "yarn@3.2.4",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npocccties/chiloportal.git"
  },
  "scripts": {
    "api": "run-s api:clean api:build",
    "api:build": "openapi2aspida -i ../backend/doc/openapi.yaml",
    "api:clean": "rimraf api",
    "build": "run-s api build:clean build:path build:app",
    "build:app": "next build",
    "build:clean": "rimraf public/mockServiceWorker.js",
    "build:path": "pathpida --ignorePath .gitignore",
    "dev": "npm-run-all -s api dev:mock -p dev:path dev:app",
    "dev:app": "next dev",
    "dev:mock": "msw init public --no-save",
    "dev:path": "pathpida --ignorePath .gitignore --watch",
    "format": "run-s format:code format:package",
    "format:code": "prettier --write .",
    "format:package": "fixpack",
    "lint": "next lint",
    "start": "next start",
    "update": "yarn up '*' && yarn up --recursive '*' && yarn dedupe"
  }
}
